      "cell_type": "code",
      "source": [
        "%%writefile invoice_utils.py\n",
        "\"\"\"\n",
        "invoice_utils.py\n",
        "Simple utilities for invoice processing (example).\n",
        "Author: Your Name\n",
        "\"\"\"\n",
        "\n",
        "from typing import List, Dict\n",
        "def parse_line_item(line: str) -> Dict:\n",
        "    \"\"\"\n",
        "    Parse a line item string like \"2 x Coffee - 150.00\" into structured dict.\n",
        "    Args:\n",
        "        line: line item string\n",
        "    Returns:\n",
        "        dict with keys: qty(int), description(str), unit_price(float), line_total(float)\n",
        "    \"\"\"\n",
        "    try:\n",
        "        left, price_str = line.rsplit('-', 1)\n",
        "        price = float(price_str.strip())\n",
        "        qty_part, desc = left.split('x', 1)\n",
        "        qty = int(qty_part.strip())\n",
        "        description = desc.strip()\n",
        "        return {\"qty\": qty, \"description\": description, \"unit_price\": price, \"line_total\": qty * price}\n",
        "    except Exception as e:\n",
        "        print(f\"Warning: failed to parse line '{line}': {e}\")\n",
        "        return {\"qty\": 0, \"description\": line, \"unit_price\": 0.0, \"line_total\": 0.0}\n",
        "\n",
        "def invoice_total(line_items: List[Dict]) -> float:\n",
        "    \"\"\"\n",
        "    Compute subtotal from list of parsed line item dicts.\n",
        "    \"\"\"\n",
        "    return sum(item.get(\"line_total\", 0.0) for item in line_items)\n",
        "\n",
        "if __name__ == \"__main__\":\n",
        "    sample = [\"2 x Coffee Beans - 156.00\", \"1 x Mug - 150.00\"]\n",
        "    parsed = [parse_line_item(l) for l in sample]\n",
        "    print(\"Parsed:\", parsed)\n",
        "    print(\"Subtotal:\", invoice_total(parsed))\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "1zheCH44yfpt",
        "outputId": "941a3377-c9ea-447f-f99d-7378fefc995f"
      },
      "execution_count": 26,
      "outputs": [
        {
